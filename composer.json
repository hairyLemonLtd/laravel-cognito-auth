{
    "name": "hairylemon-ltd/laravel-cognito-auth",
    "description": "An auth provider for Laravel for AWS Cognito.",
    "keywords": [
        "BlackBits",
        "HairyLemon",
        "AWS Cognito",
        "Auth Provider"
    ],
    "homepage": "https://github.com/hairylemon-ltd/laravel-cognito-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Carl Bowden",
            "email": "carl@hairylemon.co.nz",
            "homepage": "https://hairylemon.co.nz",
            "role": "Developer"
        },
        {
            "name": "Oliver Heck",
            "email": "oliver.heck@blackbits.io",
            "homepage": "https://blackbits.io",
            "role": "Developer"
        },
        {
            "name": "Andreas Przywara",
            "email": "andreas.przywara@blackbits.io",
            "homepage": "https://blackbits.io",
            "role": "Developer"
        },
        {
            "name": "Adrian Raeuchle",
            "email": "adrian.raeuchle@blackbits.io",
            "homepage": "https://blackbits.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "8.*",
        "illuminate/config": "8.*",
        "illuminate/auth": "8.*",
        "illuminate/http": "8.*",
        "laravel/fortify": "1.*",
        "aws/aws-sdk-php": "^3.61"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "hairyLemonLtd\\LaravelCognitoAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hairyLemonLtd\\LaravelCognitoAuth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "hairyLemonLtd\\LaravelCognitoAuth\\CognitoAuthServiceProvider"
            ]
        }
    }
}
